  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "watch": "cd client && webpack --mode=development --watch",
    "build:dev": "cd client && webpack --mode=development",
    "build:prod": "cd client && webpack --mode=production",
    "serve:dev": "nodemon server/src/",
    "serve:aws": "pm2 start ./server/src/index.js",
    "serve:heroku": "node ./server/src/index.js",
    "start:aws": "npm run build:prod && npm run serve:aws",
    "postinstall": "npm run build:prod",
    "start:heroku": "npm run serve:heroku",
    "build": "npm run build:prod"
  },
